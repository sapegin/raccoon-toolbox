{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Raccoon Toolbox",
  "mainBinaryName": "Raccoon Toolbox",
  "version": "0.1.0",
  "identifier": "me.sapegin.toolbox",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Raccoon Toolbox",
        "width": 1024,
        "height": 768,
        "label": "main"
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.13"
    }
  }
}
