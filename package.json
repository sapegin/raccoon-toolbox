{
  "name": "raccoon-toolbox",
  "private": true,
  "version": "0.1.0",
  "author": {
    "name": "Artem Sapegin",
    "url": "https://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapegin/raccoon-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/sapegin/raccoon-toolbox/issues"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:app": "npm run tauri dev",
    "build": "tsc && vite build && vite build --ssr && node --experimental-strip-types scripts/prerender.ts",
    "build:app": "npm run tauri build -- --bundles app",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint . --cache --fix && cargo check --manifest-path src-tauri/Cargo.toml",
    "typecheck": "tsc --noEmit",
    "format": "prettier --log-level warn --write \"**/*.{js,cjs,mjs,ts,tsx,css,json,md}\" && cargo fmt --manifest-path src-tauri/Cargo.toml",
    "pretest": "npm run lint",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "prepare": "husky; panda codegen",
    "generate:unicode": "node --experimental-strip-types scripts/generate-unicode-data.ts",
    "generate:tools": "node --experimental-strip-types scripts/generate-tools.ts"
  },
  "dependencies": {
    "@codemirror/commands": "6.10.0",
    "@codemirror/lang-css": "6.3.1",
    "@codemirror/lang-html": "6.4.11",
    "@codemirror/lang-javascript": "6.2.4",
    "@codemirror/lang-json": "6.0.2",
    "@codemirror/lang-xml": "6.1.0",
    "@codemirror/lang-yaml": "6.1.2",
    "@codemirror/language": "6.11.3",
    "@codemirror/search": "6.5.11",
    "@codemirror/state": "6.5.2",
    "@lezer/highlight": "1.2.3",
    "@pandacss/dev": "^1.5.1",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@types/diff": "7.0.2",
    "colord": "2.9.3",
    "diff": "8.0.2",
    "js-md5": "0.8.3",
    "js-yaml": "4.1.0",
    "nano-staged": "0.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "7.9.6",
    "tamia": "^7.2.1"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "3.4.2",
    "@tauri-apps/cli": "^2",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@unicode/unicode-17.0.0": "1.6.14",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-tamia": "^9.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.2"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,ts,tsx,css,json,md}": "prettier --write",
    "*.{js,cjs,mjs,ts,tsx}": "eslint --cache --fix",
    "*.rs": "cargo fmt --manifest-path src-tauri/Cargo.toml --"
  }
}
