{
  "name": "raccoon-toolbox",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:app": "npm run tauri dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint . --cache --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --log-level warn --write \"**/*.{js,cjs,mjs,ts,tsx,astro,css,json,md}\"",
    "pretest": "npm run lint",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "prepare": "husky; panda codegen"
  },
  "dependencies": {
    "@codemirror/lang-css": "6.3.1",
    "@codemirror/lang-javascript": "6.2.4",
    "@codemirror/lang-json": "6.0.2",
    "@codemirror/language": "6.11.3",
    "@codemirror/search": "6.5.11",
    "@codemirror/state": "6.5.2",
    "@lezer/highlight": "1.2.3",
    "@pandacss/dev": "^1.4.3",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@types/diff": "7.0.2",
    "colord": "2.9.3",
    "diff": "8.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "7.9.5",
    "tamia": "^7.2.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.38.0",
    "eslint-config-tamia": "^9.7.0",
    "eslint-plugin-astro": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.12"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx,astro,css,json,md}": "prettier --write",
    "*.{js,cjs,mjs,ts,tsx,astro}": "eslint --cache --fix"
  }
}
